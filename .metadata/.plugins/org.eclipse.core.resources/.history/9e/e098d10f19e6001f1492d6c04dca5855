	private String nameOfTheTeam, nameOfManager, testingType;
	
	public Tester() {
		this.setNameOfTheTeam(nameOfTheTeam);
		this.setNameOfManager(nameOfManager);
		this.setTestingType(testingType);
	}

	public String getNameOfTheTeam() {
		return nameOfTheTeam;
	}

	public String setNameOfTheTeam(String nameOfTheTeam) {
		return this.nameOfTheTeam = nameOfTheTeam;
	}

	public String getNameOfManager() {
		return nameOfManager;
	}

	public String setNameOfManager(String nameOfManager) {
		return this.nameOfManager = nameOfManager;
	}

	public String getTestingType() {
		return testingType;
	}

	public String setTestingType(String testingType) {
		return this.testingType = testingType;
	}
}