package pkgOrgStructure;  // name of package

public class Employee {
//	    public String name;  // instance variables
//	    public int id;
	
//	 	public Employee(String name, int id) {  // parameterized constructor
//	        this.name = name;
//	        this.id = id;
//	    }
		
		private int empId, empNm, empAge, empAadharCrd, empSal, empContactNbr, empExp;
		private String empPanCrd, empDept;
		
		public Employee() {
			this.setEmpId(empId);
			this.setEmpNm(empNm);
			this.setEmpAge(empAge);
			this.setEmpPanCrd(empPanCrd);
			this.setEmpAadharCrd(empAadharCrd);
			this.setEmpDept(empDept);
			this.setEmpSal(empSal);
			this.setEmpContactNbr(empContactNbr);
			this.setEmpExp(empExp);
		}
		
		public int getEmpId() {
			return empId;
		}
		
		public void setEmpId(int empId) {
			this.empId = empId;
		}
		
		public int getEmpNm() {
			return empNm;
		}
		
		public void setEmpNm(int empNm) {
			this.empNm = empNm;
		}
		
		public int getEmpAge() {
			return empAge;
		}
		
		public void setEmpAge(int empAge) {
			this.empAge = empAge;
		}
		
		public String getEmpPanCrd() {
			return empPanCrd;
		}
		
		public void setEmpPanCrd(String empPanCrd) {
			this.empPanCrd = empPanCrd;
		}
		
		public int getEmpAadharCrd() {
			return empAadharCrd;
		}
		
		public void setEmpAadharCrd(int empAadharCrd) {
			this.empAadharCrd = empAadharCrd;
		}
		
		public String getEmpDept() {
			return empDept;
		}
		
		public void setEmpDept(String empDept) {
			this.empDept = empDept;
		}
		
		public int getEmpSal() {
			return empSal;
		}
		
		public void setEmpSal(int empSal) {
			this.empSal = empSal;
		}
		
		public int getEmpContactNbr() {
			return empContactNbr;
		}
		
		public void setEmpContactNbr(int empContactNbr) {
			this.empContactNbr = empContactNbr;
		}
		
		public int getEmpExp() {
			return empExp;
		}
		
		public void setEmpExp(int empExp) {
			this.empExp = empExp;
		}	
}
