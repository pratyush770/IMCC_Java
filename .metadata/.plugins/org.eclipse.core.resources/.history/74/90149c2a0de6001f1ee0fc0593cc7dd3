package pkgOrgStructure;  // name of package

public class EmployeeDemo 
{
    public static void main(String[] args) 
    {   	
        Manager m = new Manager("", 0, 0);  // Manager object creation
        Developer d = new Developer("", 0, "");  // Developer object creation
        Tester t = new Tester("", 0, 0);  // Tester object creation
        
        m.setName("Pratyush Majumdar");   // set manager name
        m.setId(1);  // set manager id
        m.setBudget(100000);  // set manager budget
        System.out.println("The manager details are as follows...");  // display manager details
        System.out.println("Manager name is: " + m.getName());  // getter method usage
        System.out.println("Manager id is: " + m.getId());
        System.out.println("The budget is: " + m.getBudget());
        System.out.println();  // for new line
        
        d.setName("Saad Sayyed");  // set developer name
        d.setId(2);  // set developer id
        d.setPosition("Senior");  // set developer position
        System.out.println("The developer details are as follows...");  // display developer details
        System.out.println("Developer name is: " + d.getName());
        System.out.println("Developer id is: " + d.getId());
        System.out.println("Developer position is: " + d.getPosition());
        System.out.println();
        
        t.setName("Om Lakade");  // set tester name
        t.setId(3);  // set tester id
        t.setNumTestCases(100);  // set number of test cases
        System.out.println("The tester details are as follows...");  // display tester details
        System.out.println("Tester name is: " + t.getName());
        System.out.println("Tester id is: " + t.getId());
        System.out.println("The number of testcases handled by the tester are: " + t.getNumTestCases());
    }
}