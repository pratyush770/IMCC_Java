package pkgPolymorphism;
import java.util.*;

public class PolymorphismDemo {
	public static void main(String [] args)
	{
		String chc;
		Printer prntObj = new Printer();
		System.out.print("Enter YES if you want a colour print else enter NO: ");
		Scanner sc = new Scanner(System.in);
		chc = sc.next();
		if (chc.equalsIgnoreCase("YES")) {
			prntObj.printPaper(true);
		}
		else if(chc.equalsIgnoreCase("NO")) {
			prntObj.printPaper();
		}
		else {
			System.out.println("Your input does not match");
		}
	}
}