package pkgInheritance;

public class Animal {
	private int age;  // private variable named age
	private String gender;  // private variable named gender
 	private double weight;  // private variable named weight
	
	public void eat() {  // eat method
		System.out.println("Eat class");
	}
	
	public void sleep() {  // sleep method
		System.out.println("Sleep class");
	}
	
	public void details() {  // details method
		System.out.println("Details class");
	}
	
	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getGender() {
		return gender;
	}

	public void setGender(String gender) {
		this.gender = gender;
	}

	public double getWeight() {
		return weight;
	}

	public void setWeight(double weight) {
		this.weight = weight;
	}
}
