package pkgParameterPassing;

public class Marksheet{
	double totalMarks, percentage;
	String grade;
	
	public void calculateGrade(double c1, double c2, double c3, double c4, double c5) {
//		super(c1, c2, c3, c4, c5);
		totalMarks = c1 + c2 + c3 + c4 + c5; 
		percentage = (totalMarks * 100) / 500;
		
		if (percentage <= 50) {
			grade = "F";
		}
		else if (percentage > 50 && percentage <=60) {
			grade = "P";
		}
		else if (percentage >60 && percentage <=70) {
			grade = "G";
		}
		else if (percentage > 70 && percentage <=75) {
			grade = "E";
		}
		else if (percentage > 75) {
			grade = "O";
		}
		
		
	}

}
