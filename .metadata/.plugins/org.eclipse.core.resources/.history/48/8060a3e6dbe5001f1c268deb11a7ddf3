package pkgOrgStructure;  // name of package

public class Manager extends Employee {
    private int budget;

    public Manager(String name, int id, int budget) {
        super(name, id);
        this.budget = budget;
    }

	public int getBudget() {
		return budget;
	}

	public void setBudget(int budget) {
		this.budget = budget;
	}
}
