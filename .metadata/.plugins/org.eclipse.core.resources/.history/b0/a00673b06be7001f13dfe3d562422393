package pkgParameterPassing;

public class ArgumentPassingDemo {
	public static void main(String [] args) {
		Student s = new Student("2401121", "Pratyush", "FY-MCA", "Sem2", 90, 90, 90, 90, 90);
		Marksheet m = new Marksheet();
		m.calculateGrade(s.course1, s.course2, s.course3, s.course4, s.course5);
		m.printMarksheet();
		
	}

}
